@Article{mueller2007,
  Title                    = {Dopamine activity in the occipital and temporal cortices of rats: dissociating effects of sensory but not pharmacological stimulation.},
  Author                   = {Christian P M\"uller and Joseph P Huston},
  Journal                  = {Synapse},
