  Keywords                 = {Amygdala,Amygdala: cytology,Amygdala: physiology,Animals,Basal Ganglia,Basal Ganglia: cytology,Basal Ganglia: physiology,Behavior, Animal,Behavior, Animal: physiology,Dopamine,Dopamine: metabolism,Dopamine: physiology,Hypothalamus,Hypothalamus: cytology,Hypothalamus: physiology,Models, Biological,Neurons,Neurons: cytology,Neurons: metabolism,Neurons: physiology,Reward,Serotonin,Serotonin: metabolism,Serotonin: physiology},
