@Article{Everitt2001,
  Title                    = {The neuropsychological basis of addictive behaviour.},
  Author                   = {Everitt, B. J. and Dickinson, A. and Robbins, T. W.},
  Journal                  = {Brain Res Brain Res Rev},
  Year                     = {2001},
  Month                    = {Oct},
  Number                   = {2-3},
  Pages                    = {129--138},
  Volume                   = {36},

  Abstract                 = {The argument advanced in this review is that drug addiction can be understood in terms of normal learning and memory systems of the brain which, through the actions of chronically self-administered drugs, are pathologically subverted, thereby leading to the establishment of compulsive drug-seeking habits, strengthened by the motivational impact of drug-associated stimuli and occurring at the expense of other sources of reinforcement. We review data from our studies that have utilized procedures which reveal the various influences of pavlovian stimuli on goal-directed behaviour, namely discriminated approach, pavlovian-to-instrumental transfer and conditioned reinforcement, in order to demonstrate their overlapping and also unique neural bases. These fundamental studies are also reviewed in the context of the neural and psychological mechanisms underlying drug-seeking behaviour that is under the control of drug-associated environmental stimuli. The ways in which such drug-seeking behaviour becomes compulsive and habitual, as well as the propensity for relapse to drug-seeking even after long periods of relapse, are discussed in terms of the aberrant learning set in train by the effects of self-administered drugs on plastic processes in limbic cortical-ventral striatal systems.},
  Institution              = {Department of Experimental Psychology, University of Cambridge, Downing Street, CB2 3EB, Cambridge, UK. bje10@cus.cam.ac.uk},
  Keywords                 = {Animals; Compulsive Behavior, pathology/physiopathology/psychology; Conditioning (Psychology), physiology; Dopamine, metabolism; Humans; Learning, physiology; Limbic System, drug effects/pathology/physiopathology; Nerve Net, drug effects/pathology/physiopathology; Substance-Related Disorders, physiopathology/psychology},
  Medline-pst              = {ppublish},
  Owner                    = {vitay},
  Pii                      = {S0165017301000881},
  Pmid                     = {11690609},
  Timestamp                = {2015.09.18}
}

@Article{Eyny2003,
  Title                    = {{Opposing roles of D1 and D2 receptors in appetitive conditioning.}},
  Author                   = {Eyny, Yaniv S and Horvitz, Jon C},
  Journal                  = {J. Neurosci.},
  Year                     = {2003},
