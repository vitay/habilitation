@Article{Goldman-Rakic1992,
  Title                    = {{The anatomy of dopamine in monkey and human prefrontal cortex.}},
  Author                   = {Goldman-Rakic, P S and Lidow, M S and Smiley, J F and Williams, M S},
  Journal                  = {J. Neural Transm. Suppl.},
  Year                     = {1992},
  Month                    = jan,
  Pages                    = {163--77},
  Volume                   = {36},

  Abstract                 = {This chapter reviews recent evidence establishing the comparable organization of dopamine afferents and dopaminergic receptors in the human and monkey prefrontal cortex. Light microscopy using a dopamine-specific antibody reveals that the dopamine innervation in the human prefrontal cortex exhibits a distinct bilaminar distribution with dense bands of fibers in the upper and deeper strata of the cortex, closely resembling the patterning of dopamine fibers in the monkey prefrontal cortex. Also, EM-immunohistochemistry has now revealed identical synaptic complexes both in human and monkey. In both species, dopamine axons from symmetric synapses predominantly on the spines of pyramidal cells. In many cases, the same spine is apposed by an asymmetric, putatively excitatory synapse. Finally, both in human and monkey prefrontal cortex, the dopamine D1-specific ligand, 3H-SCH23390, and the D2-specific ligand, H3-raclopride, label binding sites in laminar positions which match the location of the densest dopamine innervation. These results indicate that the organization of the cortical dopamine system is essentially the same in macaque monkey and human and that the nonhuman primate is a suitable animal model for analysis of dopamine function in prefrontal cortex.},
  ISSN                     = {0303-6995},
  Keywords                 = {Afferent Pathways,Afferent Pathways: physiology,Animals,Autoradiography,Axons,Axons: ultrastructure,Dopamine,Dopamine: physiology,Frontal Lobe,Frontal Lobe: chemistry,Frontal Lobe: physiology,Haplorhini,Haplorhini: physiology,Humans,Neurons,Neurons: chemistry,Receptors, Dopamine,Receptors, Dopamine: analysis},
  Owner                    = {vitay},
  Pmid                     = {1527516},
  Timestamp                = {2015.08.30},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/1527516}
}

