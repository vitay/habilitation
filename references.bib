  Timestamp                = {2015.10.02}
}

@Article{Takeda2005,
  Title                    = {Active maintenance of associative mnemonic signal in monkey inferior temporal cortex.},
  Author                   = {Masaki Takeda and Yuji Naya and Ryoko Fujimichi and Daigo Takeuchi and Yasushi Miyashita},
  Journal                  = {Neuron},
  Year                     = {2005},
