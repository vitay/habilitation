}

@Article{Tobler2005,
  Title                    = {{Adaptive coding of reward value by dopamine neurons.}},
  Author                   = {Tobler, Philippe N and Fiorillo, Christopher D and Schultz, Wolfram},
  Journal                  = {Science},
