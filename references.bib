@Article{Groshek2005,
  Title                    = {{Amygdala central nucleus function is necessary for learning, but not expression, of conditioned auditory orienting.}},
  Author                   = {Groshek, Frank and Kerfoot, Erin and McKenna, Vanessa and Polackwich, Alan S and Gallagher, Michela and Holland, Peter C},
  Journal                  = {Behav. Neurosci.},
