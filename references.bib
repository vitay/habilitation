@Article{Balsam2002,
  Title                    = {{Timing at the Start of Associative Learning}},
  Author                   = {Balsam, Peter D. and Drew, Michael R. and Yang, Cynthia},
  Journal                  = {Learn. Motiv.},
  Year                     = {2002},
  Number                   = {1},
  Pages                    = {141--155},
  Volume                   = {33},

  Abstract                 = {Some theories of associative learning imply that time plays a fundamental role in the acquisition process. Consistent with these theories, this paper presents evidence that the time from the onset of a conditioned stimulus (CS) until presentation of the unconditioned stimulus (US) is learned very rapidly at the start of training. We report two autoshaping studies and a study on aversive conditioning in goldfish in which we examine timing at the start of conditioning. We also review data from a number of other conditioning preparations, including fear-potentiated startle, appetitive conditioning in rats, and eyeblink conditioning in rabbits, that report conditioned response (CR) timing early in training. Acquisition speed and the very first expressions of conditioned responding often show sensitivity to the time of US presentation. In instances where temporal control is slowly expressed, it is likely due to performance factors, not to slow learning about time. In fact, the learning about time may be a necessary condition for associative learning.},
  File                     = {::},
  Keywords                 = {Key Words: associative learning,Pavlovian conditioning,timing},
  Owner                    = {vitay},
  Timestamp                = {2015.08.30},
