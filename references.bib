@Article{Usuda1998,
  Title                    = {{Efferent projections of the nucleus accumbens in the rat with special reference to subdivision of the nucleus: biotinylated dextran amine study.}},
  Author                   = {Usuda, I and Tanaka, K and Chiba, T},
